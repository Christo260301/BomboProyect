@model List<BomboProyect.Models.Insumos>

@{ Layout = null; }

<input type="hidden" value="0" name="ContElement" id="ContElement" />
<input type="hidden" value="" name="ContElmeJson" id="ContElmeJson" />
<h5>Insumos Seleccionados</h5>
<table class="table">
    <thead class="table-secondary">
        <tr>
            <th>Nombre</th>
            <th>Cantidad Total</th>
            <th>Acction</th>
        </tr>
    </thead>
    <tbody id="tbContainer">
    </tbody>
</table>

@if (ViewBag.validInsumoList != null)
{
    if (ViewBag.validInsumoList)
    {
        <div>
            <span class="text-danger">La cantidad de los insumos debe ser mayor a 0</span>
        </div>
    }

}
